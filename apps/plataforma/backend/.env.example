# Porta na qual a API NestJS irá escutar
PORT=4000

# URL de conexão com o banco de dados PostgreSQL. Substitua valores conforme sua configuração
DATABASE_URL=postgresql://usuario:senha@localhost:5432/qats

# URL de conexão com o Redis utilizado para cache e filas. Em ambientes Docker Compose, use redis://redis:6379
REDIS_URL=redis://localhost:6379

# Chave secreta utilizada para assinar JWTs
JWT_SECRET=change-me-secret

# Configurações do OAuth GitHub
GITHUB_CLIENT_ID=<coloque seu client id>
GITHUB_CLIENT_SECRET=<coloque seu client secret>
GITHUB_REDIRECT_URI=http://localhost:5173/auth/callback